<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.mytopics.Meeting.MyTopicMeetingFragment">

    <EditText
        android:id="@+id/titleMeetingMyTopic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="TextView"
        android:enabled="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <DatePicker
        android:id="@+id/dayMeetingMyTopic"
        android:layout_width="310dp"
        android:layout_height="89dp"
        android:layout_marginTop="28dp"
        android:calendarViewShown="false"
        android:datePickerMode="spinner"
        android:enabled="false"
        android:clickable="false"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/titleMeetingMyTopic" />

    <TimePicker
        android:id="@+id/hourMeetingMyTopic"
        android:layout_width="312dp"
        android:layout_height="93dp"
        android:layout_marginTop="28dp"
        android:enabled="false"
        android:text="TextView"
        android:clickable="false"
        android:timePickerMode="spinner"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dayMeetingMyTopic" />

    <EditText
        android:id="@+id/descMeetingMyTopic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="TextView"
        android:enabled="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/placeMeetingMyTopic" />

    <TextView
        android:id="@+id/cptYesMeeting"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="64dp"
        android:layout_marginTop="36dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10" />

    <TextView
        android:id="@+id/cptNoMeeting"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.711"
        app:layout_constraintStart_toEndOf="@+id/cptYesMeeting"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <Button
        android:id="@+id/editMeetingMyTopic"
        android:layout_width="49dp"
        android:layout_height="46dp"
        android:layout_marginStart="72dp"
        android:drawableTop="@drawable/edit"
        android:text="@string/edit"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cptYesMeeting"
        app:layout_constraintVertical_bias="0.24" />

    <Button
        android:id="@+id/deleteMeeting"
        android:layout_width="75dp"
        android:layout_height="36dp"
        android:background="@color/design_default_color_error"
        android:text="@string/delete"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.752"
        app:layout_constraintStart_toEndOf="@+id/editMeetingMyTopic"
        app:layout_constraintTop_toBottomOf="@+id/cptNoMeeting"
        app:layout_constraintVertical_bias="0.617" />

    <EditText
        android:id="@+id/placeMeetingMyTopic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="TextView"
        android:enabled="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/hourMeetingMyTopic" />

    <Button
        android:id="@+id/saveMeetingMyTopic"
        android:layout_width="107dp"
        android:layout_height="50dp"
        android:layout_marginBottom="24dp"
        android:text="@string/saveChangesAccount"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/deleteMeeting"
        app:layout_constraintHorizontal_bias="0.244"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editMeetingMyTopic"
        app:layout_constraintVertical_bias="0.263" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="84dp"
        android:layout_marginTop="28dp"
        android:text="@string/present"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/descMeetingMyTopic" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="140dp"
        android:layout_marginTop="28dp"
        android:text="@string/notPresent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.193"
        app:layout_constraintStart_toEndOf="@+id/textView10"
        app:layout_constraintTop_toBottomOf="@+id/descMeetingMyTopic" />

</androidx.constraintlayout.widget.ConstraintLayout>